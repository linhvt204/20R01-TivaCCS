/*******************************************************************************
 *                              Includes  & Defines
 *******************************************************************************/
//  #include <example.h>
#include <user_init/InitInc.h>

//  #define example 0x04
/*******************************************************************************
 *                              Global Variables
 *******************************************************************************/
//  uint32_t example;
/*******************************************************************************
 *                              Function Prototypes
 *******************************************************************************/
//  void example(void);
/*******************************************************************************
 *                              Main Program
 *******************************************************************************/

int main(void)
{
    IntMasterDisable();
    Init_SysCtl(40);
    PinoutSet();
    // Init Peripherals
    Init_Led(RED | GREEN | BLUE);
    Init_Hibernation();
    IntMasterEnable();
    while (1)
    {
        LEDS ^= BLUE;
        HibernateRequest();
    }
}

